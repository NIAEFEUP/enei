{
  "name": "@enei/utils",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch --preserveWatchOutput --diagnostics false",
    "turbo:build": "turbo run build",
    "turbo:dev": "turbo run dev"
  },
  "devDependencies": {
    "@enei/typescript-config": "workspace:*"
  },
  "exports": {
    "./defer": {
      "types": "./src/defer.ts",
      "default": "./dist/defer.js"
    },
    "./dynamic-import": {
      "types": "./src/dynamic-import.ts",
      "default": "./dist/dynamic-import.js"
    },
    "./types": {
      "types": "./src/types.ts",
      "default": "./dist/types.js"
    }
  }
}